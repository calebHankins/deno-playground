    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "promises - clone",
            "program": "${workspaceFolder}/promises/index.js",
            "cwd": "${workspaceFolder}/promises/",
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "promises - cleanup",
            "program": "${workspaceFolder}/promises/cleanup.js",
            "cwd": "${workspaceFolder}/promises/",
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/index.js"
        }
    ]
}
